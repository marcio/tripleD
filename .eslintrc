{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [],
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "quotes": [2, "single"],
    "comma-dangle": [2, "always-multiline"],
    "max-len": ["error", 140],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],

  }
}